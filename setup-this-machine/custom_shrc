alias la="ls -AlF"
alias rm="trash"
alias dclk="xdg-open"
alias py="python"
alias py3="python3"
alias shed="gedit $HOME/.bashrc"
alias shrc="source $HOME/.bashrc"

alias pull="git pull --rebase"
alias gadd="git add"
alias gcomm="git commit -m"
alias gst="git status"
alias gdiff="git diff"
alias gch="git checkout"
alias glog="git log"
